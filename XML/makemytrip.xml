<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "testng-1.0.dtd">
<suite name="Makemytrip">

	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url" value="https://makemytrip.com/" />
	<parameter name="captureScreenshots" value="true" />
	<parameter name="enableLogs" value="true" />
	<parameter name="enableExtentReports" value="true" />

	<listeners>
		<listener class-name="listeners.Listners" />
		<listener class-name="listeners.Report_log" />
	</listeners>


	<test name="Makemytrip" enabled="true">
		<packages>
			<package name="makemytrip" />
		</packages>
	</test>


</suite> <!-- Suite -->

